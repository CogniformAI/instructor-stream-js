{
  "name": "@cogniformai/instructor-stream",
  "version": "0.0.2",
  "private": true,
  "description": "streaming-first structured outputs for llms",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "oxlint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "build": "pnpm -r run build",
    "ci:publish": "pnpm --filter @cogniformai/instructor-stream run build && pnpm changeset publish",
    "publish:dry-run": "dotenv -- bash -c 'pnpm run format && pnpm run lint && pnpm run test && pnpm run build'",
    "publish:local": "dotenv -- pnpm -r publish --access public --no-git-checks",
    "release:patch": "pnpm run version:patch && pnpm run publish",
    "release:minor": "pnpm run version:minor && pnpm run publish",
    "release:major": "pnpm run version:major && pnpm run publish",
    "pre-push": "pnpm run format && pnpm run lint && pnpm run test && pnpm run build",
    "clean": "pnpm -r run clean",
    "dev": "pnpm -r run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cogniformai/instructor-stream-js.git"
  },
  "keywords": [
    "llm",
    "structured",
    "outputs",
    "zod"
  ],
  "author": "David Robertson <info@drobertson.pro> (https://x.com/davidalexr987)",
  "contributors": [
    "Dimitri Kennedy",
    "Jason Liu"
  ],
  "maintainers": [
    "David Robertson <info@drobertson.pro>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cogniformai/instructor-stream-js/issues"
  },
  "homepage": "https://github.com/cogniformai/instructor-stream-js#readme",
  "dependencies": {
    "zod": "catalog:",
    "zod-validation-error": "catalog:"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "catalog:",
    "@changesets/cli": "catalog:",
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@prettier/plugin-oxc": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "dotenv": "catalog:",
    "dotenv-cli": "catalog:",
    "openai": "catalog:",
    "oxlint": "catalog:",
    "prettier": "latest",
    "tiktoken": "catalog:",
    "ts-inference-check": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "openai": "catalog:",
    "zod": "catalog:",
    "zod-validation-error": "catalog:"
  }
}
