{
  "name": "@cogniformai/instructor-stream-js",
  "version": "0.0.0",
  "private": true,
  "description": "streaming-first structured outputs for llms",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup",
    "lint": "eslint \"**/*.{js,json,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky || true",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "tsup && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cogniformai/instructor-stream-js.git"
  },
  "keywords": [
    "llm",
    "structured",
    "outputs",
    "zod"
  ],
  "author": "David Robertson <info@drobertson.pro> (https://x.com/davidalexr987)",
  "contributors": [
    "Dimitri Kennedy",
    "Jason Liu"
  ],
  "maintainers": [
    "David Robertson <info@drobertson.pro>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cogniformai/instructor-stream-js/issues"
  },
  "homepage": "https://github.com/cogniformai/instructor-stream-js#readme",
  "dependencies": {
    "@types/ramda": "^0.30.2",
    "llm-polyglot": "^2.6.0",
    "ramda": "^0.31.3",
    "zod-to-json-schema": "^3.24.6",
    "zod-validation-error": "^3.4.0"
  },
  "peerDependencies": {
    "openai": ">=4.58.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.33.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/bun": "^1.0.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.12",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "openai": "4.78.1",
    "prettier": "latest",
    "ts-inference-check": "^0.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}
