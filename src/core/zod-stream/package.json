{
  "name": "@cogniformai/zod-stream",
  "version": "3.0.0",
  "description": "A client for node or the browser to generate and consume streaming json",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "bun test --coverage --verbose",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "directory": "src/core/zod-stream",
    "type": "git",
    "url": "git+https://github.com/cogniformai/instructor-stream-js.git"
  },
  "keywords": [
    "llm",
    "structured output",
    "streaming",
    "react",
    "zod"
  ],
  "license": "MIT",
  "author": "David Robertson <info@drobertson.pro> (https://x.com/davidalexr987)",
  "homepage": "https://github.com/cogniformai/instructor-stream-js",
  "private": true,
  "dependencies": {
    "@cogniformai/schema-stream": "^3.2.2",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "openai": "5.10.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/eslint": "^8.56.1",
    "eslint": "^8.56.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "ramda": "^0.31.3",
    "zod": "^4.0.5"
  }
}
